<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
    
    <hibernate-mapping>
    <class name="org.fi.ordershipping.AgentOrderDetails" table="orderandagentdetails">
    <id name="agentId" column="agentId" type="java.lang.int"/>
    
    <property name="agentName" column="agentName" type="java.lang.String"></property>
    <property name="address" column="address" type='java.lang.String'></property>
    <property name="detailOfProduct" column="detailOfProduct" type='java.lang.String'></property>
    
    
    </class>
    </hibernate-mapping>
    
